{
  "expo": {
    "name": "Glow - Look your best",
    "slug": "glow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F80ED"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.supitspete.glow",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.supitspete.glow"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://glowapp.com/"
        }
      ],
      "expo-font",
      "sentry-expo"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://glowapp.com/"
      }
    },
    "owner": "supitspete",
    "runtimeVersion": "1.0.5",
    "updates": {
      "url": "https://u.expo.dev/08ef2857-8897-4721-a044-c53d2a95ddf5"
    }
  }
}
